nat : cotype.
a : nat.
b : nat.



stream : cotype.
cons : nat -> stream -> stream.


stream_of : nat -> stream = [A] cons A (stream_of A).

stream_of_a : stream = cons a (stream_of_a).
stream_of_b : stream = cons b (stream_of_b).



stream_merge_alternating : stream -> stream -> stream -> cotype.
%mode stream_merge_alternating + + -.
stream_merge_alternating/def : 
    stream_merge_alternating Y W Z ->
    stream_merge_alternating (cons A W) Y (cons A Z).


    


main : stream -> cotype.
%mode main -.
main/def : stream_merge_alternating (stream_of a) stream_of_b Z ->
    main Z.






