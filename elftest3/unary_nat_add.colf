nat : cotype.
zero : nat.
succ : nat -> nat.

add : nat -> nat -> nat -> cotype.
%mode add + + -.
add/0 : {B : nat} add zero B B.
add/s : {A : nat} {B : nat} {C : nat} 
    add A B C -> add (succ A) B (succ C).

two : nat -> cotype.
%mode two -.
two/0 : two (succ (succ zero)).


main : nat -> cotype.
%mode main -.
main/def : {A : nat} {B : nat} {C : nat} 
    two A -> two B -> add A B C -> main C.